<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Quick_News_App.Resource_Dictionaries">

	<Color x:Key="Form_BackgroundColor">#f7f7f7</Color>
	<Color x:Key="Form_BorderColor">#000000</Color>

	<Color x:Key="TextColor">#393E46</Color>

	<Color x:Key="ButtonColor_Normal">#115EBA</Color>
	<Color x:Key="ButtonColor_Hover">#0b3a75</Color>

	<Color x:Key="ButtonColor_Text">#f7f7f7</Color>
	<SolidColorBrush x:Key="ButtonColorBrush_Text">#f7f7f7</SolidColorBrush>

	<SolidColorBrush x:Key="ButtonColor2_Normal">#346cb2</SolidColorBrush>
	<SolidColorBrush x:Key="ButtonColor2_Hover">#2e609e</SolidColorBrush>

	<SolidColorBrush x:Key="ButtonColor3_Normal">#2d83eb</SolidColorBrush>
	<SolidColorBrush x:Key="ButtonColor3_Hover">#1675e9</SolidColorBrush>

	<SolidColorBrush x:Key="ButtonColor4_Normal">#4491ee</SolidColorBrush>
	<SolidColorBrush x:Key="ButtonColor4_Hover">#1469d2</SolidColorBrush>

	<Color x:Key="TextBoxColor_Outline">#115EBA</Color>
	<Color x:Key="TextBoxColor_Hint">#b3b3b3</Color>
	<Color x:Key="TextBoxColor_Foreground">#393E46</Color>

	<BitmapImage x:Key="MainBackgorundImage" UriSource="..\Icons\lightBG.jpg"></BitmapImage>
	
	<BitmapImage x:Key="LogoSource" UriSource="..\Icons\LogoLightMode.png"></BitmapImage>
	
	<BitmapImage x:Key="icon_Politics" UriSource="..\Icons\blue_Politics.png"></BitmapImage>
	<BitmapImage x:Key="icon_Sports" UriSource="..\Icons\blue_Sports.png"></BitmapImage>
	<BitmapImage x:Key="icon_Business" UriSource="..\Icons\blue_Business.png"></BitmapImage>
	<BitmapImage x:Key="icon_Science" UriSource="..\Icons\blue_Science.png"></BitmapImage>
	<BitmapImage x:Key="icon_Automobile" UriSource="..\Icons\blue_Automobile.png"></BitmapImage>
	<BitmapImage x:Key="icon_Fashion" UriSource="..\Icons\blue_Fashion.png"></BitmapImage>
	<BitmapImage x:Key="icon_Health" UriSource="..\Icons\blue_Health.png"></BitmapImage>
	<BitmapImage x:Key="icon_Education" UriSource="..\Icons\blue_Education.png"></BitmapImage>

	<!-- 
	Lightest = #f7f7f7
	Light Second = #F0EDED
	Light Third = #E6E3E3
	Blue = #115EBA
	Pink = #E94560
	
	Black For Text = #222831
	-->
	
</ResourceDictionary>